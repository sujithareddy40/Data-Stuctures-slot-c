#include<stdio.h>
int main()
{
	int n,i,a[5];
	printf("enter number of elements in an array");
	scanf("%d",&n);
	printf("enter elements into an array :\n");
	for(i=0;i<n;i++)
	{
	scanf("%d",&a[i]);
}
printf("even numbers in the array are: \n");
for(i=0;i<n;i++)
{
	if(a[i]%2==0)
	printf("%d \n",a[i]);
}
printf("odd numbers in the array are:\n");
for(i=0;i<n;i++)
{
	if(a[i]%2!=0)
	printf("%d \n",a[i]);
}
return 0;
}
